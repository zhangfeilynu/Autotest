    <!-- ............................................... -->
    
     <LoginPage name="登录页面">
         <suime>logo</suime><!-- 页面顶端导航：随米classname -->
         <print>html/body/div[1]/ul[1]/li[1]/a</print><!-- 页面顶端导航：打印 -->
         <library>html/body/div[1]/ul[1]/li[2]/a</library><!-- 页面顶端导航：文库 -->
         <mydocuments>html/body/div[1]/ul[1]/li[3]/a</mydocuments><!-- 页面顶端导航：我的文档 -->
         <housemaster>active</housemaster><!-- 页面顶端导航：成为楼长classname -->
         <cellphone>div>input[type='text']</cellphone><!-- 手机号输入框css -->
         <password>div>input[type='password</password><!-- 密码输入框 css-->
         <login>button-block</login><!-- 登录按钮classname -->
